public class Main {
    public static void main(String[] args) {
        String str = "I love my family";
        String result = "";
        for (int i = 0; i < str.length(); i++) {
            if (str.charAt(i) != ' ') {
                result += str.charAt(i);
            }
        }
        System.out.println(result);
    }
}
