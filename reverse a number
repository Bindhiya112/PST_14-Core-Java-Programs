public class Main {
    public static void main(String[] args) {
        int num = 4932;
        int reverse = 0;
        while (num!= 0) {
            int lastDigit = num % 10;        
            reverse = reverse * 10 + lastDigit;
            num = num / 10;                 
        }
        System.out.println("Reversed number = " + reverse);
    }
}
