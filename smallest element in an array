package PST;
public class smallestnumber {
	    public static void main(String[] args) {
	        int[] arr = {8, 2, 15, 1, 10};
	        int smallest = arr[0];
	        for (int i = 1; i < arr.length; i++) {
	            if (arr[i] < smallest) {
	                smallest = arr[i];
	            }
	        }
	        System.out.println("Smallest element: " + smallest);
	    }
	}



