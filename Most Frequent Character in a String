public class Main {
    public static void main(String[] args) {
        String str = "banana";
        int maxCount = 0;
        char mostFrequent = ' ';
        for (int i = 0; i < str.length(); i++) {
            char ch = str.charAt(i);
            int count = 0;
            for (int j = 0; j < str.length(); j++) {
                if (str.charAt(j) == ch) {
                    count++;
                }
            }
            if (count > maxCount) {
                maxCount = count;
                mostFrequent = ch;
            }
        }
        System.out.println("Most frequent character: " + mostFrequent);
    }
}
