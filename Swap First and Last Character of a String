public class Main {
    public static void main(String[] args) {
        String str = "Bindhiya";
        char[] ch = str.toCharArray();
        char temp = ch[0];
        ch[0] = ch[ch.length - 1];
        ch[ch.length - 1] = temp;
        String swapped = new String(ch);
        System.out.println("Swapped string: " + swapped);
    }
}
